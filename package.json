{
	"name": "NightPDF",
	"version": "0.2.0",
	"description": "Dark Mode for PDFs",
	"main": "app.js",
	"scripts": {
		"start": "electron .",
		"dist": "electron-builder"
	},
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"electron": "^8.3.0",
		"electron-builder": "^22.6.0"
	},
	"dependencies": {
		"nouislider": "^14.5.0"
	},
	"build": {
		"appId": "com.elesoft.NightPDF",
		"productName": "NightPDF",
		"fileAssociations": [
			{
				"ext": "pdf",
				"icon": "icon.icns",
				"role": "viewer",
				"isPackage": false
			}
		],
		"mac": {
			"identity": null,
			"icon": "icon.iconset",
			"category": "public.app-category.productivity"
		}
	}
}
